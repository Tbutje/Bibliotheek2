<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/categorieen.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/categorieEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/categorieEdit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>terug</from-outcome>
            <to-view-id>/categorieen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/categorieen.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>naarboeken</from-outcome>
            <to-view-id>/boeken.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boeken.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>categorieen</from-outcome>
            <to-view-id>/categorieen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boeken.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>boekedit</from-outcome>
            <to-view-id>/boekEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boeken.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>exemplaartoevoegen</from-outcome>
            <to-view-id>/exemplaarEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boeken.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>uitleneninnemen</from-outcome>
            <to-view-id>/uitlenenInnemen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boeken.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>titeltoevoegen</from-outcome>
            <to-view-id>/boekEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/uitlenenInnemen.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>boekedit</from-outcome>
            <to-view-id>/boekEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
        
    <navigation-rule>
        <from-view-id>/uitlenenInnemen.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>naarboeken</from-outcome>
            <to-view-id>/boeken.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/uitlenenInnemen.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>naaruitlenen</from-outcome>
            <to-view-id>/boekUitlenen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/exemplaarEdit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>naarboeken</from-outcome>
            <to-view-id>/boeken.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/boekEdit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>uitleneninnemen</from-outcome>
            <to-view-id>/uitlenenInnemen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boekEdit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>naarboeken</from-outcome>
            <to-view-id>/boeken.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/boekUitlenen.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>naaruitleneninnemen</from-outcome>
            <to-view-id>/uitlenenInnemen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    
</faces-config>
